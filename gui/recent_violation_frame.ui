<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.LabelFrame" id="recent_violation_frame">
    <property name="height">300</property>
    <property name="relief">flat</property>
    <property name="takefocus">false</property>
    <property name="text" translatable="yes">Recent Violation</property>
    <property name="width">300</property>
    <layout manager="grid">
      <property name="propagate">True</property>
    </layout>
    <child>
      <object class="tk.Message" id="violation_message_title">
        <property name="background">#ff2424</property>
        <property name="font">{Haettenschweiler} 20 {}</property>
        <property name="foreground">#dddd00</property>
        <property name="justify">center</property>
        <property name="relief">raised</property>
        <property name="text" translatable="yes">WARNING ! !
</property>
        <property name="width">400</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.31</property>
          <property name="relwidth">1.0</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Message" id="violation_message_type">
        <property name="anchor">w</property>
        <property name="aspect">0</property>
        <property name="background">#ffff80</property>
        <property name="borderwidth">2</property>
        <property name="font">{Microsoft YaHei UI} 14 {}</property>
        <property name="foreground">#151515</property>
        <property name="highlightbackground">#ffff20</property>
        <property name="highlightcolor">#ffff20</property>
        <property name="highlightthickness">1</property>
        <property name="justify">left</property>
        <property name="relief">groove</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">
Type: Lane Cross Violation
</property>
        <property name="width">300</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="bordermode">ignore</property>
          <property name="height">0</property>
          <property name="relheight">0.16</property>
          <property name="relwidth">1.0</property>
          <property name="relx">0.0</property>
          <property name="rely">0.26</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Message" id="violaton">
        <property name="anchor">w</property>
        <property name="background">#ffff80</property>
        <property name="cursor">arrow</property>
        <property name="font">{@Yu Gothic UI Light} 12 {}</property>
        <property name="relief">groove</property>
        <property name="text" translatable="yes">Description:  A vehicle at (200,300,400,500) commenced a left lane cross violation.

Saving the violation video.</property>
        <property name="width">290</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.65</property>
          <property name="relwidth">1.0</property>
          <property name="relx">0.0</property>
          <property name="rely">0.38</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
